{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx", // Enables React 17+ JSX transform
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ESNext",
    "strict": true, // Optional: for stricter type-checking
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
, "store/authStore.jsx"  ]
}